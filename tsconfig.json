{
  "compilerOptions": {
    "types": ["node"],
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "lib": ["DOM", "ES5", "ES6", "ES7"],
    "jsx": "react-jsx"
  },
  "include": ["**/*.ts"]
}
